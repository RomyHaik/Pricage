{
  "manifest_version": 3,
  "name": "Privage",
  "version": "4.3",
  "description": "Analyze and calculate average prices across various websites",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}